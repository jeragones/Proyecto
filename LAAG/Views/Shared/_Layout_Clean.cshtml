<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title - Lab. Análisis Agronómicos</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />

    @Styles.Render("~/Content/css/bootstrap.min.css")
    @Styles.Render("~/Content/css/sb-admin.css")
    @Styles.Render("~/Content/css/plugins/morris.css")
    @Styles.Render("~/Content/font-awesome-4.1.0/css/font-awesome.min.css")

    @Scripts.Render("~/Scripts/plugins/morris/morris-data.js")
</head>
<body>
    <div>
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/Home/Index">Lab. Análisis Agronómicos</a>
            </div>
        </nav>
        <div id="page-wrapper">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->
    <div class="for-full-back " id="footer">
        2014 www.dominio.com | Todos los Derechos Reservados
    </div>

    <script src="~/Scripts/jquery-2.1.1.js"></script>
    <script src="~/Scripts/jquery-2.1.1.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/morris/raphael.min.js")"></script>
    <script src="@Url.Content("~/Scripts/plugins/morris/morris.min.js")"></script>

    <script src="@Url.Content("~/Scripts/custom/index.js")"></script>

    @RenderSection("Scripts", false)
</body>
</html>