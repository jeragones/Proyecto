@model LAAG.Facturas

@{
    ViewBag.Title = "Registro de Muestras";
}



@using (Html.BeginForm(null, null, FormMethod.Post, new { name = "formCreateCost", id = "formCreateCost" })) 
 {
    @Html.AntiForgeryToken()
    

    <div class="container">
        <h2>Ingreso de Muestras</h2>
        <div class="row">
            <div class="progress" id="progress1">
                <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"></div>
            </div>
        </div>
            <div class="col-lg-6">
                <div id="step-1" class="row setup-content">
                    @Html.Partial("_CreateFormPartial1")
                </div>
                <div id="step-2" class="row setup-content">
                    @Html.Partial("_CreateFormPartial2")
                </div> 
            </div>
    </div>
    <div class="form-group">
        <input type="submit" id="btnSave" class="btn btn-primary pull-right" value="Guardar" />
    </div>
}

@section Scripts{
    @Scripts.Render("~/Scripts/plugins/jquery-1.11.1.min.js")
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/custom/costo.js"></script>
}

