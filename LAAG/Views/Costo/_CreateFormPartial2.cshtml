@model LAAG.Facturas


<fieldset>

    <div class="form-group">
        @Html.LabelFor(model => model.Campo)
        @Html.TextBoxFor(model => model.Campo, new { @class = "txtField form-control" })
        @Html.ValidationMessageFor(model => model.Campo)
    </div>

    <div class="row">
        <div class="col-xs-6 text-left">
            <div class="form-group">
                <div class="row">
                    @Html.LabelFor(model => model.Categoria)
                    @Html.DropDownListFor(model => model.Categoria, new SelectList(new List<String> { }), new { @class = "cmbCategory form-control" })
                    @Html.ValidationMessageFor(model => model.Categoria)
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    @Html.LabelFor(model => model.Analisis)
                    @Html.DropDownListFor(model => model.Analisis, new SelectList(new List<String> { }), new { @class = "cmbAnalysis form-control" })
                    @Html.ValidationMessageFor(model => model.Analisis)
                </div>
            </div>
            <table id="tblAnalisis">

            </table>
        </div>
    </div>
    <br />
    
    <div class="row text-right">
        <div class="form-group">
            <input type="button" id="btnBack" class="btn btn-primary pull-right" value="Volver" />
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.Costo)
        @Html.TextBoxFor(model => model.Costo, new { @class = "lblCost form-control", disabled = "disabled" })
        @Html.ValidationMessageFor(model => model.Costo)
    </div>
</fieldset>





